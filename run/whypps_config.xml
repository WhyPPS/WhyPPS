<?xml version="1.0" encoding="UTF-8" ?>
<framework>

    <localServer>
        <server autoDistribution="true" enable="true">
            <port>2683</port>
        </server>
    </localServer>

    <debug enable="true">
        <debugkey>admin</debugkey>
    </debug>

    <certificates>
        <dir path="${runPath}\certificates"/>
    </certificates>

    <authority>
        <privateKey enable="true">
            <cipher file="${runPath}/key/PBEKey.key"/>
            <password file="${runPath}/key/psw.txt"/>
        </privateKey>
        <setting effectiveTime="1800"/>
    </authority>

</framework>